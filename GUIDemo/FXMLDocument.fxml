<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guidemo.FXMLDocumentController">
   <children>
      <HBox fillHeight="false" layoutX="14.0" layoutY="14.0" prefHeight="240.0" prefWidth="920.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="240.0" prefWidth="180.0" spacing="10.0">
               <children>
                  <Label text="CheckBox Example" />
                  <CheckBox fx:id="pepperoniCheckBox" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="pineappleCheckBox" mnemonicParsing="false" text="Pineapple" />
                  <CheckBox fx:id="baconCheckBox" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#pizzaOrderButtonPushed" text="Order Pizza" />
                  <Label fx:id="pizzaOrderLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="240.0" prefWidth="180.0" spacing="10.0">
               <children>
                  <Label text="ChoiceBox Example" />
                  <ChoiceBox fx:id="choiceBox" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favorite Fruit" />
                  <Label fx:id="choiceBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="240.0" prefWidth="180.0" spacing="10.0">
               <children>
                  <Label text="ComboBox Example" />
                  <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdated" />
                  <Label fx:id="comboBoxLabel" text="Label" />
               </children>
            </VBox>
            <VBox prefHeight="240.0" prefWidth="180.0" spacing="10.0">
               <children>
                  <Label text="RadioButton Example" />
                  <RadioButton fx:id="phpButton" mnemonicParsing="false" onAction="#radioButtonWasUpdated" text="PHP" />
                  <RadioButton fx:id="JavaButton" mnemonicParsing="false" onAction="#radioButtonWasUpdated" text="Java" />
                  <RadioButton fx:id="CsharpButton" mnemonicParsing="false" onAction="#radioButtonWasUpdated" text="C#" />
                  <RadioButton fx:id="CppButton" mnemonicParsing="false" onAction="#radioButtonWasUpdated" text="C++" />
                  <Label fx:id="radioButtonLabel" text="Label" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox layoutY="240.0" prefHeight="240.0" prefWidth="920.0" spacing="20.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="ListView Example" />
                  <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#listViewButtonPushed" text="Selected Gear -&gt;" />
               </children>
            </StackPane>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label text="TextArea Example" />
                  <TextArea fx:id="golfTextArea" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button layoutX="127.0" layoutY="199.0" mnemonicParsing="false" onAction="#changeSceneButton" text="Change View" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
